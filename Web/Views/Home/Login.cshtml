<body class="login"> <!-- Some dialogs etc. -->
    <!-- The loading box -->
    <div id="loading-overlay"></div>
    <div id="loading"> <span>Loading...</span> </div>
    <!-- End of loading box -->
    <!--------------------------------->
    <!-- Now, the page itself begins -->
    <!--------------------------------->

    <!-- The header containing the logo -->
    <div style="height: 200px">
     
    </div>
    <!-- End of header -->
    <!-- The container of the sidebar and content box -->
    <section id="login" class="container_12 clearfix">
        <form action="@Url.Action("Autenticar", "Home", null, "http")" method="post" class="box validate">
            <div class="header">
                <h2><span class="icon icon-lock"></span>Iniciar Sesión</h2>
            </div>
            <div class="content">
                <!-- Login messages -->
                <div class="login-messages">
                    @if (string.IsNullOrEmpty(ViewBag.mensaje) )
                    {
                        <div class="message welcome">Bienvenido a SOPHIX COMERCIAL</div>
                        <div class="message failure">Credentiales Invalidos.</div>
                    }
                    else
                    {
                        <div class="message welcome" style="display: none;">Bienvenido a SOPHIX COMERCIAL</div>
                        <div class="message failure" style="display: block;">@ViewBag.mensaje</div>
                    }
                </div>
                <!-- The form -->
                <div class="form-box">
                    <div class="row">
                         <label for="login_name"> <strong>Usuario</strong></label>
                         <div> <input tabindex="1" class="required noerror" name="login_name" id="login_name" type="text"> </div>
                    </div>
                    <div class="row">
                        <label for="login_pw"> <strong>Clave</strong></label>
                        <div> <input tabindex="2" class="required noerror" name="login_pw" id="login_pw" type="password"> </div>
                    </div>
                    <div class="row">
                         <label for="login_oficina"> <strong>Oficina</strong></label>
                        <div>
                            @* <select id="cboOficina" name="cboOficina"></select>*@
                            @Html.DropDownList("cboOficina")
                        </div>
                    </div>
                </div>
                <!-- End of .form-box --> </div>
            <!-- End of .content -->
            <div class="actions">
                <div class="left">
                    <div class="rememberme"> <input tabindex="4" name="login_remember" id="login_remember" checked="checked" type="checkbox">
                    <label for="login_remember">Recordarme?</label> </div>
                </div>
                <div class="right">
                    <input tabindex="3" value="Ingresar" name="login_btn"  id="login_btn" type="submit"> 
                </div>
            </div>
            <!-- End of .actions --> </form>

        <!-- End of form --> </section>
    <!-- Spawn $$.loaded -->
    
